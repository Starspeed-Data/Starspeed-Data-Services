{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "First_Source",
						"type": "DatasetReference"
					},
					"name": "DataLakeTable1"
				},
				{
					"dataset": {
						"referenceName": "First_Table",
						"type": "DatasetReference"
					},
					"name": "SQLTable1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "ModifyColumns1",
					"description": "Autogenerated by data preview actions"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ID as string,",
				"          Name as string,",
				"          Age as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DataLakeTable1",
				"source(output(",
				"          ID as integer,",
				"          Name as string,",
				"          Age as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SQLTable1",
				"DataLakeTable1, ModifyColumns1 join(DataLakeTable1@ID == DataLakeTable1@ID,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 derive(ID = ERROR_FUNCTION('')) ~> derivedColumn1",
				"SQLTable1 derive(ID = toString(ID)) ~> ModifyColumns1"
			]
		}
	}
}