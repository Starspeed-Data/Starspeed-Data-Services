					"SFTPAeroCRS_properties_typeProperties_userName": {
						"value": "[parameters('SFTPAeroCRS_properties_typeProperties_userName')]"
					},
					"starspeeddatalake_properties_typeProperties_url": {
						"value": "[parameters('starspeeddatalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
